{* Latte template *}

{block content}

<h1 n:block=title>Seznam useru</h1>
<table>
    <thead>
        <tr>
            <td><b>jmeno</b></td>
            <td><b>login</b></td>
			<td></td>
        </tr>
    </thead>
    <tbody>
        {foreach $users as $user}
            <tr>
                <td>{$user->jmeno}</td>
                <td>{$user->login}</td>
				<td><a n:href="useredit:delete $user->ID">Smaz</a></td>
            </tr>
        {/foreach}    
    </tbody> 
</table>